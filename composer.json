{
    "name": "siraken/php-template",
    "type": "project",
    "description": "PHP Template",
    "license": "UNLICENSED",
    "require": {
        "php": "^7.4|^8.0",
        "nesbot/carbon": "^2.62",
        "fakerphp/faker": "^1.20",
        "filp/whoops": "^2.14",
        "vlucas/phpdotenv": "^5.4",
        "monolog/monolog": "^3.2",
        "illuminate/database": "^9.30"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Core/functions.php",
            "app/Config/Database.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --testdox"
        ]
    },
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "optimize-autoloader": true
    }
}
